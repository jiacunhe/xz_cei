<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:id="@+id/home_page_re"
	android:background="@drawable/welcome_background">
	<ImageView
		android:src="@drawable/mainpage_top_part"
		android:layout_width="fill_parent"
		android:id="@+id/homepage_dzb_top"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true"></ImageView>
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:id="@+id/relativeLayout1">
		<ImageView
			android:id="@+id/home_ceinet"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/homepage_botton_button_3" />
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_witsea"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_button_2"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_ceinet"></ImageView>
		<RelativeLayout
			android:layout_height="wrap_content"
			android:id="@+id/home_main_num"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_num"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_witsea">
			<TextView
				android:visibility="invisible"
				android:gravity="center"
				android:background="@drawable/home_page_main_num"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:id="@+id/home_page_main_num"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" />
		</RelativeLayout>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_announcement"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_button_1"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_main_num"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/imageView1"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_left"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_announcement"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_personcenter"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_button_4"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_ceinet"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_disclaimer"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_button_5"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_personcenter"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/imageView2"
			android:layout_width="wrap_content"
			android:background="@drawable/homepage_botton_right"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_disclaimer"></ImageView>
	</RelativeLayout>

	<RelativeLayout
		android:id="@+id/homepage_re_mid"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_above="@+id/relativeLayout1"
		android:layout_below="@+id/homepage_dzb_top"
		android:layout_centerHorizontal="true">

		<ImageView
			android:src="@drawable/mainpage_login_part"
			android:layout_width="wrap_content"
			android:id="@+id/homepage_dzb_login"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerHorizontal="true" />
		<TextView
			android:paddingTop="65dip"
			android:layout_width="wrap_content"
			android:id="@+id/homepage_dzb_login_text"
			android:layout_height="wrap_content"
			android:text="登   陆"
			android:textSize="20sp"
			android:textColor="#000000"
			android:textStyle="bold"
			android:layout_centerVertical="true"
			android:layout_centerHorizontal="true" />
		<TextView
			android:ellipsize="marquee"
			android:focusable="true"
			android:marqueeRepeatLimit="marquee_forever"
			android:focusableInTouchMode="true"
			android:scrollHorizontally="true"
			android:layout_below="@+id/homepage_dzb_login_text"
			android:paddingTop="5dip"
			android:singleLine="true"
			android:id="@+id/homepage_dzb_login_text_2"
			android:layout_width="130dip"
			android:gravity="center"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:textSize="30sp"
			android:textColor="#000000"
			android:textStyle="bold" />
		<Button
		android:visibility="gone"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/homepage_dzb_zhzx"
			android:text="综合资讯"
			android:layout_below="@+id/homepage_dzb_login"
			android:layout_centerHorizontal="true"></Button>
		<Button
			android:visibility="invisible"
			android:id="@+id/button1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_marginTop="55dp"
			android:layout_toLeftOf="@+id/homepage_dzb_login_text"
			android:text="Button" />
		<Button
			android:id="@+id/homepage_dzb_yjbg"
			android:visibility="invisible"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:text="研究报告" />

	</RelativeLayout>

	<RelativeLayout
		android:id="@+id/homepage_re_left"
		android:layout_toLeftOf="@+id/homepage_re_mid"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/homepage_dzb_top"
		android:layout_alignBottom="@+id/homepage_re_mid">
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/homepage_left_mid"
			android:layout_width="wrap_content"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			android:background="@drawable/business_left_default"
			android:layout_marginRight="100dip" />
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/homepage_left_botton"
			android:layout_alignParentRight="true"
			android:layout_width="wrap_content"
			android:layout_below="@+id/homepage_left_mid"
			android:background="@drawable/business_left_default"
			android:layout_marginTop="50dip" />
		<ImageView
			android:layout_alignParentRight="true"
			android:layout_height="wrap_content"
			android:id="@+id/homepage_left_top"
			android:layout_width="wrap_content"
			android:layout_above="@+id/homepage_left_mid"
			android:background="@drawable/business_left_default"
			android:layout_marginBottom="50dip" />
	</RelativeLayout>
	<RelativeLayout
		android:id="@+id/homepage_re_right"
		android:layout_toRightOf="@+id/homepage_re_mid"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/homepage_dzb_top"
		android:layout_above="@+id/relativeLayout1">
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/homepage_right_mid"
			android:layout_alignParentLeft="true"
			android:layout_width="wrap_content"
			android:layout_centerVertical="true"
			android:background="@drawable/business_right_default"
			android:layout_marginLeft="100dip" />
		<ImageView
			android:layout_alignParentLeft="true"
			android:layout_height="wrap_content"
			android:id="@+id/homepage_right_top"
			android:layout_width="wrap_content"
			android:layout_above="@+id/homepage_right_mid"
			android:background="@drawable/business_right_default"
			android:layout_marginBottom="50dip" />
		<ImageView
			android:layout_alignParentLeft="true"
			android:layout_height="wrap_content"
			android:id="@+id/homepage_right_botton"
			android:layout_width="wrap_content"
			android:layout_below="@+id/homepage_right_mid"
			android:background="@drawable/business_right_default"
			android:layout_marginTop="50dip" />
	</RelativeLayout>
</RelativeLayout>  