<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="40dip">
	<ImageView android:id="@+id/iv_goodbg_hand"
	android:paddingLeft="10dip"
	android:layout_gravity="center"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/bg_prit_left1" />

	<TextView android:id="@+id/tv_pritbg_text"
	android:layout_gravity="center"
	android:paddingLeft="20dip"
	android:textSize="20dip"
	android:textColor="@color/white"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true" android:text="body" />


</LinearLayout>
