<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/welcome_background">

	<ImageView
		android:id="@+id/homepage_dzb_top"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_alignParentTop="true"
		android:src="@drawable/mainpage_top_part" />

	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:id="@+id/botton"
        android:visibility="invisible">
		<ImageView
			android:id="@+id/home_ceinet"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/homepage_botton_button_3" />
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_witsea"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_button_2"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_ceinet"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_main_num"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_num"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_witsea"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_announcement"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_button_1"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_main_num"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/imageView1"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_left"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@+id/home_announcement"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_personcenter"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_button_4"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_ceinet"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/home_disclaimer"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_button_5"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_personcenter"></ImageView>
		<ImageView
			android:layout_height="wrap_content"
			android:id="@+id/imageView2"
			android:layout_width="wrap_content"
			android:src="@drawable/homepage_botton_right"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/home_disclaimer"></ImageView>
	</RelativeLayout>

	<RelativeLayout
		android:layout_centerHorizontal="true"
		android:layout_centerVertical="true"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/login_1"
			android:id="@+id/imageView3"
		>
			<RelativeLayout
				android:layout_width="100dip"
				android:layout_height="100dip"
				android:layout_centerInParent="true"
			>
				<Button
					android:id="@+id/back_back"
					android:background="@drawable/back_back"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentBottom="true"
					android:layout_centerHorizontal="true" />
			</RelativeLayout>
		</RelativeLayout>
		<ImageView
			android:layout_width="wrap_content"
			android:background="@drawable/login_9"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/imageView3"
			android:id="@+id/imageView4"></ImageView>
		<ScrollView
			android:layout_above="@+id/imageView99"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_below="@+id/imageView4"
			android:layout_alignLeft="@+id/imageView4"
			android:id="@+id/imageView5">
			<LinearLayout
				android:background="@drawable/login_3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:id="@id/ui_regist_etparent">
				<RelativeLayout
					android:layout_marginTop="10dip"
					android:layout_width="300dip"
					android:layout_height="wrap_content">

					<TextView
						android:id="@+id/textview111"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="用户名 :"
						android:textColor="#000000"
						android:textSize="20sp" />
					<EditText
						android:maxWidth="150dip"
						android:layout_marginLeft="10dip"
						android:layout_toRightOf="@+id/textview111"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip"
						android:singleLine="true" />
					<ImageView
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:background="@drawable/grzx_star" />
				</RelativeLayout>

				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview222"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="密码 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
						android:maxWidth="150dip"
						android:layout_marginLeft="10dip"
						android:layout_toRightOf="@+id/textview222"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip"
						android:password="true"
						android:singleLine="true" />
					<ImageView
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:background="@drawable/grzx_star" />
				</RelativeLayout>

				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview333"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="密码确认 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
						android:maxWidth="150dip"
						android:layout_marginLeft="10dip"
						android:layout_toRightOf="@+id/textview333"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip"
						android:password="true"
						android:singleLine="true" />
					<ImageView
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:background="@drawable/grzx_star" />
				</RelativeLayout>



				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview444"
						android:layout_width="100dip"
						android:gravity="right"
						android:layout_height="wrap_content"
						android:text="昵称 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
						android:maxWidth="150dip"
						android:layout_toRightOf="@+id/textview444"
						android:layout_marginLeft="10dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip"
						android:singleLine="true" />
					<ImageView
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:background="@drawable/grzx_star" />
				</RelativeLayout>

				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview555"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="电子邮箱 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
					    android:singleLine="true"
						android:maxWidth="150dip"
						android:layout_toRightOf="@+id/textview555"
						android:layout_marginLeft="10dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip" />
					<ImageView
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:background="@drawable/grzx_star" />
				</RelativeLayout>

				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:layout_centerVertical="true"
						android:id="@+id/textview666"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="性别 :"
						android:textColor="#000000"
						android:textSize="20sp" />
					<RadioGroup
						android:layout_centerVertical="true"
						android:layout_marginLeft="10dip"
						android:layout_toRightOf="@+id/textview666"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:orientation="horizontal"
						android:id="@+id/lines"
						android:checkedButton="@+id/wlan">
						<RadioButton
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:id="@+id/economic_register_register_query_accurate_query_radio"
							android:text="男"
							android:textColor="#000000"
							android:layout_marginLeft="10dip"
							android:checked="true" />
						<RadioButton
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:id="@+id/economic_register_register_qussssery_vague_query_radio"
							android:text="女"
							android:textColor="#000000"
							android:layout_marginLeft="10dip" />
					</RadioGroup>
				</RelativeLayout>
				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview777"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="证件类型 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
					    android:singleLine="true"
						android:maxWidth="150dip"
						android:layout_toRightOf="@+id/textview777"
						android:layout_marginLeft="10dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/regist_6"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip" />
				</RelativeLayout>
				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview888"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="证件号码 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
					    android:singleLine="true"
						android:maxWidth="150dip"
						android:layout_toRightOf="@+id/textview888"
						android:layout_marginLeft="10dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip" />
				</RelativeLayout>
				<RelativeLayout
					android:layout_width="300dip"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dip">

					<TextView
						android:id="@+id/textview999"
						android:gravity="right"
						android:layout_width="100dip"
						android:layout_height="wrap_content"
						android:text="电话 :"
						android:textColor="#000000"
						android:textSize="20sp" />

					<EditText
					    android:singleLine="true"
						android:maxWidth="150dip"
						android:layout_toRightOf="@+id/textview999"
						android:layout_marginLeft="10dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/ui_login_etborder"
						android:paddingLeft="10dip" />
				</RelativeLayout>
				<RelativeLayout
					android:layout_marginTop="10dip"
					android:layout_width="300dip"
					android:layout_height="wrap_content">
					<ImageView
						android:layout_centerInParent="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@id/ui_regist_login"
						android:background="@drawable/ui_regist_regist" />
				</RelativeLayout>
			</LinearLayout>
		</ScrollView>

		<ImageView
			android:id="@+id/imageView99"
			android:layout_width="wrap_content"
			android:background="@drawable/login_4"
			android:layout_height="wrap_content"
			android:layout_alignBottom="@+id/imageView3"
			android:layout_alignRight="@+id/imageView5"></ImageView>
		<ImageView
			android:layout_width="wrap_content"
			android:background="@drawable/login_5"
			android:layout_height="wrap_content"
			android:layout_below="@+id/imageView4"
			android:layout_toRightOf="@+id/imageView5"
			android:id="@+id/imageView6"></ImageView>
		<ImageView
			android:id="@id/ui_regist_regist"
			android:layout_width="wrap_content"
			android:background="@drawable/login_6"
			android:layout_height="wrap_content"
			android:layout_below="@+id/imageView6"
			android:layout_toRightOf="@+id/imageView5"></ImageView>
	</RelativeLayout>

</RelativeLayout>