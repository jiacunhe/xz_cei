<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background" >

    <RelativeLayout
        android:id="@+id/econ_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/top" >

        <ImageView
            android:id="@+id/econ_data_dataquary_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:layout_marginLeft="10.0dip"
            android:visibility="gone"
            android:src="@drawable/back" />
        <!--
    <ImageView android:src="@drawable/econ_sz"
	android:layout_centerVertical="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:gravity="center" android:layout_alignParentRight="true"
		android:layout_marginRight="10.0dip" android:id="@+id/imageView1"></ImageView>
        -->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="50dip"
            android:layout_toRightOf="@+id/econ_data_dataquary_back"
            style="@style/yangshi_pad_title"
			android:text="数据查询" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/econ_part_bottom"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/econ_data_bottom_bg"
        android:paddingLeft="100.0dip"
        android:paddingRight="100.0dip" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:orientation="horizontal"
            android:paddingTop="10.0dip"
            android:weightSum="7.0" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_home1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_zx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_data1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_sz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_kx1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_zz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_zs1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_fx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_fx" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_zb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_zb1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" >

                <ImageView
                    android:id="@+id/econ_data_dataquary_sj"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.0"
                    android:background="@drawable/econ_data_cx1" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/econ_part_bottom"
        android:layout_below="@id/econ_title"
        android:layout_marginLeft="30dip"
        android:layout_marginRight="30dip"
        android:layout_marginTop="30dip"
        android:background="@drawable/jjsj_back_wai"
        android:padding="70dip" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="@drawable/jjsj_back_nei" >

            <ImageView
                android:id="@+id/econ_search_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="50dip"
                android:background="@drawable/econ_search" >
            </ImageView>

            <RelativeLayout
                android:id="@+id/shujuchaxun_re_use"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/econ_search_iv"
                android:layout_marginTop="10dip"
                android:background="@drawable/econ_search_back"
                android:paddingLeft="30dip"
                android:paddingRight="30dip" >

                <ImageView
                    android:id="@+id/econ_search_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/econ_search_btn" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/econ_search_btn"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip"
                    android:weightSum="5" >

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/shujuchaxun_data1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/econ_spinner"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:text="类别"
                            android:textSize="20dip" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/shujuchaxun_data2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/econ_spinner"
                            android:gravity="center"
                             android:text="行业"
                            android:textColor="@color/black"
                            android:textSize="20dip" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/shujuchaxun_data3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                             android:text="名称"
                            android:background="@drawable/econ_spinner"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:textSize="20dip" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/shujuchaxun_data4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                             android:text="时间"
                            android:layout_centerInParent="true"
                            android:background="@drawable/econ_spinner"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:textSize="20dip" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/shujuchaxun_data5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                             android:text="其他"
                            android:layout_centerInParent="true"
                            android:background="@drawable/econ_spinner"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:textSize="20dip" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_below="@+id/shujuchaxun_re_use"
                android:layout_marginTop="10dip"
                android:paddingLeft="30dip"
                android:paddingRight="30dip" >

                <ImageView
                    android:id="@+id/econ_search_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/econ_search_btn"
                    android:visibility="invisible" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/econ_search_btn"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip"
                    android:weightSum="5" >

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <ListView
                            android:id="@+id/shujuchaxun_datalist1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:tag="1"
                            android:visibility="invisible" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <ListView
                            android:id="@+id/shujuchaxun_datalist2"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:tag="2"
                            android:visibility="invisible" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <ListView
                            android:id="@+id/shujuchaxun_datalist3"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:tag="3"
                            android:visibility="invisible" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <ListView
                            android:id="@+id/shujuchaxun_datalist4"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:tag="4"
                            android:visibility="invisible" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <ListView
                            android:id="@+id/shujuchaxun_datalist5"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:tag="5"
                            android:visibility="invisible" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>