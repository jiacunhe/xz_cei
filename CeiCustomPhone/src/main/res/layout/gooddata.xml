<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#f0f4f6">
	<RelativeLayout
		android:id="@+id/econ_data_good_tops"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/phone_study_topbg"
		android:layout_alignParentTop="true">
		<ImageView
			android:id="@+id/econ_good_tops_text1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_alignParentLeft="true"
			android:layout_margin="15dp"
			android:src="@drawable/econ_data_imgjj" />
		<ImageView
			android:id="@+id/econ_good_tops_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerInParent="true"
			android:layout_margin="10dp"
			android:src="@drawable/econ_data_jcdata" />

		<ImageView
			android:id="@+id/econ_good_tops_tv2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:layout_margin="10dp"
			android:visibility="gone"
			android:layout_marginRight="15dp"
			android:src="@drawable/econ_data_img_sousuo"
			android:textSize="10dp" />

	</RelativeLayout>
	<LinearLayout
		android:id="@+id/econ_good_bottoms"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:weightSum="4.0"
		android:background="@drawable/phone_study_bottombg"
		android:layout_alignParentBottom="true">
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0">
			<ImageView
				android:id="@+id/econ_good_bottons_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_kx" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0">
			<ImageView
				android:id="@+id/econ_good_bottons_2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_zs" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0">
			<ImageView
				android:id="@+id/econ_good_bottons_3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_fx1" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0">
			<ImageView
				android:id="@+id/econ_good_bottons_4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_zb" />
		</RelativeLayout>
	</LinearLayout>
	<ListView
		android:id="@+id/econ_good_list_content"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@id/econ_data_good_tops"
		android:layout_above="@id/econ_good_bottoms"
		android:cacheColorHint="@null"
		android:divider="#c8c8c8"
		android:dividerHeight="1dip">
	</ListView>



</RelativeLayout>
