<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="#ffffff"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
>
	<RelativeLayout
		android:id="@+id/econ_data_search_top"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/phone_study_topbg"
		android:layout_alignParentTop="true"
	>
		<TextView
			android:id="@+id/econ_search_top_text1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_alignParentLeft="true"
			android:layout_margin="15dp"
			android:text="经济数据"
			style="@style/yangshi_phone_title_small"
			/>
		<TextView
			android:id="@+id/econ_main_top_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerInParent="true"
			android:layout_margin="10dp"
			style="@style/yangshi_phone_title_big"
			 />
	</RelativeLayout>
	<LinearLayout
		android:id="@+id/econ_data_search_bottom"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:weightSum="4.0"
		android:background="@drawable/botton_back"
		android:layout_alignParentBottom="true"
	>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0"
		>
			<ImageView
				android:id="@+id/econ_data_search_botton_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_kx" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0"
		>
			<ImageView
				android:id="@+id/econ_data_search_botton_2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_zs" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0"
		>
			<ImageView
				android:id="@+id/econ_data_search_botton_3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_fx1" />
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1.0"
		>
			<ImageView
				android:id="@+id/econ_data_search_botton_4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true"
				android:src="@drawable/econ_data_zb" />
		</RelativeLayout>
	</LinearLayout>
	<ScrollView
		android:layout_marginTop="30dip"
		android:layout_marginBottom="30dip"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_above="@+id/econ_data_search_bottom"
		android:layout_below="@+id/econ_data_search_top"
	>
		<LinearLayout
			android:gravity="center_horizontal"
			android:id="@+id/ui_regist_etparent"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal"
		>
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			>
				<EditText android:layout_weight="1" android:layout_height="wrap_content" android:background="@drawable/jjsj_search_left" android:text="按地区" android:layout_width="wrap_content" android:gravity="center" android:editable="false">
					<requestFocus></requestFocus>
				</EditText>
				<EditText
					android:background="@drawable/jjsj_search_right"
					android:singleLine="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</LinearLayout>
			<LinearLayout
				android:layout_marginTop="30dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			>
				<EditText
					android:gravity="center"
					android:text="按指标"
					android:editable="false"
					android:background="@drawable/jjsj_search_left"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				<EditText
					android:background="@drawable/jjsj_search_right"
					android:singleLine="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</LinearLayout>
			<LinearLayout
				android:layout_marginTop="30dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			>
				<EditText
					android:gravity="center"
					android:text="按时间"
					android:editable="false"
					android:background="@drawable/jjsj_search_left"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				<EditText
					android:background="@drawable/jjsj_search_right"
					android:singleLine="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</LinearLayout>
			<LinearLayout
				android:layout_marginTop="30dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			>
				<EditText
					android:gravity="center"
					android:text="按维度"
					android:editable="false"
					android:background="@drawable/jjsj_search_left"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				<EditText
					android:background="@drawable/jjsj_search_right"
					android:singleLine="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</LinearLayout>
			<LinearLayout
				android:layout_marginTop="30dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			>
				<EditText
					android:gravity="center"
					android:text="按维度"
					android:editable="false"
					android:background="@drawable/jjsj_search_left"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				<EditText
					android:background="@drawable/jjsj_search_right"
					android:singleLine="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</LinearLayout>
			<Button
				android:layout_marginTop="30dip"
				android:background="@drawable/jjsj_search_button"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />


		</LinearLayout>
	</ScrollView>
</RelativeLayout>
