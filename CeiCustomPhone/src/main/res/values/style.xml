<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="yangshi_1">
		<item name="android:textSize">28px</item>
		<item name="android:textColor">#054b93</item>
		<item name="android:maxLines">1</item>
		<item name="android:maxWidth">235px</item>
	</style>
	<style name="yangshi_2">
		<item name="android:textSize">18px</item>
		<item name="android:textColor">#3d3d3f</item>
		<item name="android:maxLength">20</item>
		<item name="android:maxLines">1</item>
	</style>
	<style name="yangshi_3">
		<item name="android:textSize">24px</item>
		<item name="android:maxLength">4</item>
		<item name="android:maxLines">1</item>
	</style>
	<style name="yangshi_4">
		<item name="android:textSize">24px</item>
	</style>
	<style name="yangshi_5">
		<item name="android:textSize">28px</item>
		<item name="android:textColor">#000000</item>
		<item name="android:maxLength">8</item>
		<item name="android:maxLines">1</item>
	</style>
	<style name="yangshi_pad_title">
		<item name="android:textSize">30px</item>
		<item name="android:textColor">#ffffff</item>
	</style>
	<style name="yangshi_phone_title_big">
		<item name="android:textSize">30px</item>
		<item name="android:textColor">#ffffff</item>
	</style>
	<style name="yangshi_phone_title_small">
		<item name="android:textSize">24px</item>
		<item name="android:textColor">#ffffff</item>
	</style>
	<style name="AppTheme" parent="android:Theme.Light" />
    <style name="phone_study_bottom_tv">
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@drawable/phone_study_bottom_tv_bg</item>
        <item name="android:paddingRight">@dimen/phone_study_bottom_tv_pad_right</item>
        <item name="android:paddingLeft">@dimen/phone_study_bottom_tv_pad_left</item>
        <item name="android:layout_marginRight">0.5dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:textSize">@dimen/phone_study_bottom_tv_textSize</item>
    </style>
    
    <style name="phone_study_bt">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">@dimen/phone_study_bt_textSize</item>
        <item name="android:layout_width">@dimen/phone_study_bt_width</item>
        <item name="android:layout_height">@dimen/phone_study_bt_height</item>
        <item name="android:background">@drawable/phone_study_bt_bg</item>
    </style>
    
    <style name="phone_study_Tab">
        <item name="android:background">@drawable/phone_study_tab_bg2</item>
        <item name="android:layout_marginLeft">@dimen/phone_study_Tab_mar_left</item>
        <item name="android:layout_marginRight">@dimen/phone_study_Tab_mar_right</item>
        <item name="android:paddingTop">@dimen/phone_study_Tab_pad_top_bot</item>
        <item name="android:paddingBottom">@dimen/phone_study_Tab_pad_top_bot</item>
        <item name="android:paddingLeft">@dimen/phone_study_Tab_pad_right_left</item>
        <item name="android:paddingRight">@dimen/phone_study_Tab_pad_right_left</item>
        <item name="android:textSize">@dimen/phone_study_Tab_textSize</item>
        <item name="android:textColor">@color/phone_study_color_White</item>
    </style>
    
    <style name="phone_study_h1">
        <item name="android:textColor">@color/phone_study_h1_color</item>
        <item name="android:textSize">@dimen/phone_study_h1_textSize</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="phone_study_h1_small">
        <item name="android:textColor">@color/phone_study_h1_small_color</item>
        <item name="android:textSize">@dimen/phone_study_h1_small_textSize</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="phone_study_h2">
        <item name="android:background">@drawable/phone_study_det_h2_bg</item>
        <item name="android:textColor">@color/phone_study_color_White</item>
        <item name="android:padding">@dimen/phone_study_h2_padding</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/phone_study_h2_textSize</item>
    </style>
    <style name="phone_study_text">
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/phone_study_text_textSize</item>
        <item name="android:textColor">@color/phone_study_color_black</item>
    </style>
    <style name="phone_study_image_detail">
        <item name="android:layout_width">@dimen/phone_study_image_detail_width</item>
        <item name="android:layout_height">@dimen/phone_study_image_detail_height</item>
    </style>
    <style name="phone_study_image_list">
        <item name="android:layout_width">@dimen/phone_study_image_list_width</item>
        <item name="android:layout_height">@dimen/phone_study_image_list_height</item>
    </style>

    <style name="phone_study_listview">
        <item name="android:paddingBottom">@dimen/phone_study_listview_paddingBottom</item>
        <item name="android:paddingTop">@dimen/phone_study_listview_paddingBottom</item>
        <item name="android:paddingLeft">@dimen/phone_study_listview_paddingleft</item>
    </style>
    <style name="phone_study_top_tv">
        <item name="android:textColor">@color/phone_study_color_White</item>
        <item name="android:textSize">@dimen/top_text_size</item>
    </style>

    <style name="phone_study_pop_tv">
        <item name="android:textColor">@color/phone_study_color_White</item>
        <item name="android:background">@drawable/phone_study_pop_bt_bg</item>
        <item name="android:textSize">@dimen/phone_study_pop_tv_textSize</item>
        <item name="android:layout_width">@dimen/phone_study_pop_tv_width</item>
        <item name="android:layout_height">@dimen/phone_study_pop_tv_height</item>
    </style>

    <style name="phone_study_gallery">
        <item name="android:textSize">@dimen/phone_study_gallery_tv</item>
    </style>

    <style name="phone_study_prload_h1">
        <item name="android:textSize">@dimen/phone_study_prload_h1_textSize</item>
        <item name="android:textColor">#054b93</item>
        <item name="android:paddingRight">@dimen/phone_study_prload_h1_paddingRight</item>
    </style>

    <style name="phone_study_prload_h2">
        <item name="android:textSize">@dimen/phone_study_prload_h2_textSize</item>
        <item name="android:textColor">#3d3d3f</item>
        <item name="android:maxLength">20</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="phone_study_login_title">
        <item name="android:textSize">@dimen/phone_study_login_title_textSize</item>
        <item name="android:textColor">@color/phone_study_color_White</item>
        <item name="android:textScaleX">1.1</item>
    </style>

    <style name="Spinner">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/spinner</item>
        <item name="android:indeterminateDuration">2000</item>
        <item name="android:indeterminateOnly">true</item>
    </style>
</resources>